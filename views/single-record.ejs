

<html>
<head>
    <title>Full Detail</title>
        <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
    <body>
            <%- include('header'); -%>
            
        <table border="1">
               
                  <tr>
                      <td>ID</td>
                      <td><%= user_array._id%></td>

                  </tr>
                  <tr>
                        <td>Name</td>
                        <td><%= user_array.user_name%></td>
  
                    </tr>
                    <tr>
                            <td>Email</td>
                            <td><a href="mailto:<%= user_array.user_email%>"><%= user_array.user_email%></a></td>
      
                        </tr>
                    <tr>
                            <td>Mobile No.</td>
                            <td><%= user_array.user_mobile%></td>
      
                        </tr>
                        <tr>
                            <td>Collage Name</td>
                            <td><%=user_array.user_collage %></td>
      
                        </tr>
                        <tr>
                            <td>University Name</td>
                            <td><%=user_array.user_university %></td>
      
                        </tr>
                        <tr>
                            <td>Team Name</td>
                            <td><%=user_array.user_team_name %></td>
      
                        </tr>
                        <tr>
                            <td>Team Id</td>
                            <td><%=user_array.user_team_id %></td>
      
                        </tr>

                        
             </table>
    </body>
</html>
