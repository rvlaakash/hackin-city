                  </tr>
                  <tr>
                        <th>email</th>
                        <td><input type="text" name="user_email"
                             value=" <%= user_array.user_email %> " required> </td>
  
                    </tr>
                  <tr>
                        <th>Mobile</th>
                        <td><input type="text" name="user_mobile"
                             value=" <%= user_array.user_mobile %> " required> </td>
  
                    </tr>
                    <tr>
                        <th>Collage Name </th>
                        <td><input type="text" name="user_collage"
                             value=" <%= user_array.user_collage %> " required> </td>
  
                    </tr>
                    <tr>
                        <th>University</th>
                        <td><input type="text" name="user_university"
                             value=" <%= user_array.user_university %> " required> </td>
  
                    </tr>
