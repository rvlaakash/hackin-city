                    </tr>
                    <tr>
                        <th>Team Id</th>
                        <td><input type="text" name="user_team_id"
                             value=" <%= user_array.user_team_id %> " required> </td>
  
                    </tr>
                    <tr>
                            
                            <td>
                            </td><td><input type="submit" class="button button2"><input type="button" value="Display"
                                onclick="window.location='/display-user';" class="button button2" style="float:right"></td>
                        </tr>
         
             </table>
        </form>
        <%- include('footer')-%>
    </body>
</html>
